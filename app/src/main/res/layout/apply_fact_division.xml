                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_fourth_second"

                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />

                </LinearLayout>

                <View
                    android:id="@+id/view_two"
                    android:layout_below="@+id/ll_fourth_row"
                    android:layout_centerHorizontal="true"
                    android:background="@color/launch_app_bar"
                    android:layout_width="70dp"
                    android:layout_height="2dp"/>
                <LinearLayout
                    android:layout_marginLeft="25dp"
                    android:layout_below="@+id/view_two"
                    android:id="@+id/ll_fifth_row"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/txt_fifth_first"
                        android:textSize="@dimen/div_divident_text_size"
                        android:visibility="visible"
                        android:text="1"
                        android:textColor="@color/launch_app_bar"

                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_fifth_second"

                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_fifth_third"

                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginLeft="7dp"
                    android:id="@+id/ll_sixth_row"
                    android:layout_below="@+id/ll_fifth_row"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/txt_sixth_minus"
                        android:text="-"
                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"


                        android:layout_marginRight="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_sixth_first"
                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_sixth_second"
                        android:textColor="@color/launch_app_bar"

                        android:textSize="@dimen/div_divident_text_size"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:textSize="@dimen/div_divident_text_size"
                        android:id="@+id/txt_sixth_third"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />

                </LinearLayout>
                <View
                    android:id="@+id/view_three"
                    android:layout_below="@+id/ll_sixth_row"
                    android:layout_marginLeft="20dp"
                    android:background="@color/launch_app_bar"
                    android:layout_width="70dp"
                    android:layout_height="2dp"/>

                <LinearLayout
                    android:layout_marginLeft="40dp"
                    android:layout_below="@+id/view_three"
                    android:id="@+id/ll_seventh_row"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/txt_seventh_first"
                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:text=" "
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_seventh_second"

                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_seventh_third"
                        android:textColor="@color/launch_app_bar"

                        android:textSize="@dimen/div_divident_text_size"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />

                </LinearLayout>
                <LinearLayout
                    android:layout_marginLeft="22dp"
                    android:id="@+id/ll_eighth_row"
                    android:layout_below="@+id/ll_seventh_row"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/txt_eigth_minus"
                        android:text="-"
                        android:textSize="@dimen/div_divident_text_size"

                        android:layout_marginRight="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_eigth_first"

                        android:textSize="@dimen/div_divident_text_size"

                        android:text=" "
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
