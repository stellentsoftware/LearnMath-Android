                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />

                </LinearLayout>
                <LinearLayout

                    android:layout_marginLeft="7dp"
                    android:id="@+id/ll_fourth_row"
                    android:layout_below="@+id/ll_third_row"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/txt_fourth_minus"
                        android:text="-"
                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:layout_marginRight="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
