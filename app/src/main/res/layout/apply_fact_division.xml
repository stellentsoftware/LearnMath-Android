                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_eigth_third"

                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />

                </LinearLayout>

                <View

                    android:id="@+id/view_four"
                    android:layout_marginLeft="25dp"
                    android:layout_below="@+id/ll_eighth_row"
                    android:background="@color/launch_app_bar"
                    android:layout_width="70dp"
                    android:layout_height="2dp"/>
                <LinearLayout
                    android:layout_marginLeft="58dp"
                    android:layout_below="@+id/view_four"
                    android:id="@+id/ll_ninth_row"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">


                    <TextView
                        android:id="@+id/txt_ninth_first"
                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_ninth_second"
                        android:textSize="@dimen/div_divident_text_size"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
