                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/txt_divisor_two"
                        android:text="1"
                        android:textColor="@color/launch_app_bar"

                        android:visibility="visible"
                        android:textSize="@dimen/div_divident_text_size"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:visibility="visible"
                        android:id="@+id/txt_divisor_one"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:textSize="@dimen/div_divident_text_size"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/division_for_cal"
                    android:layout_marginTop="10dp"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content" >


                    <TextView
                        android:id="@+id/txt_divident_one"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="15dp"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:textSize="@dimen/div_divident_text_size"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_divident_two"
                        android:layout_marginTop="10dp"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:textSize="@dimen/div_divident_text_size"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_divident_three"
                        android:layout_marginTop="10dp"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:textSize="@dimen/div_divident_text_size"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_divident_four"
                        android:layout_marginTop="10dp"
                        android:textColor="@color/launch_app_bar"

                        android:text="1"
                        android:textSize="@dimen/div_divident_text_size"
                        android:layout_width="@dimen/txt_width_apply_facts_div"
                        android:layout_height="wrap_content" />

                </LinearLayout>
                <ImageView
                    android:layout_marginTop="15dp"

                    android:id="@+id/img_up_arrow"
                    android:visibility="gone"
                    android:src="@drawable/up_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />


            </LinearLayout>
            <RelativeLayout
                android:id="@+id/ll_second_row"
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/ll_first_row"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_marginLeft="5dp"
                    android:id="@+id/txt_first_row_minus"
                    android:text="-"
                    android:textSize="@dimen/div_divident_text_size"
                    android:textColor="@color/launch_app_bar"

                    android:layout_marginRight="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/txt_first_row_first_number"
                    android:layout_toRightOf="@+id/txt_first_row_minus"
                    android:textSize="@dimen/div_divident_text_size"
                    android:textColor="@color/launch_app_bar"

                    android:text="0"
                    android:layout_width="@dimen/txt_width_apply_facts_div"
                    android:layout_height="wrap_content" />
                <ImageView
                    android:id="@+id/img_arrow_one"
                    android:layout_toRightOf="@+id/txt_first_row_first_number"
                    android:visibility="invisible"
                    android:src="@drawable/down_two"
                    android:layout_width="@dimen/txt_width_apply_facts_div"
                    android:layout_height="@dimen/img_down_arrow_one_height" />

                <ImageView
                    android:id="@+id/img_arrow_two"
                    android:layout_toRightOf="@+id/img_arrow_one"
                    android:visibility="invisible"
                    android:src="@drawable/down_one"
                    android:layout_width="@dimen/txt_width_apply_facts_div"
                    android:layout_height="@dimen/img_down_arrow_two_height" />


                <ImageView
                    android:id="@+id/img_arrow_three"
                    android:layout_toRightOf="@+id/img_arrow_two"
                    android:visibility="invisible"
                    android:src="@drawable/down_three"
                    android:layout_width="@dimen/txt_width_apply_facts_div"

                    android:layout_height="@dimen/img_down_arrow_three_height" />
                <View
                    android:id="@+id/view_one"
                    android:layout_below="@+id/txt_first_row_first_number"
                    android:layout_centerHorizontal="true"
                    android:background="@color/launch_app_bar"
                    android:layout_width="70dp"
                    android:layout_height="2dp"/>
                <LinearLayout
