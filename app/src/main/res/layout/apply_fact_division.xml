                    android:layout_marginLeft="50dp"
                    android:id="@+id/txt_quot_one"
                    android:text="1"
                    android:textSize="@dimen/div_text_size"
                    android:layout_width="@dimen/txt_width_apply_facts_div_quot"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/txt_quot_two"
                    android:textColor="@color/launch_app_bar"

                    android:textSize="@dimen/div_text_size"

                    android:text="1"
                    android:layout_width="@dimen/txt_width_apply_facts_div_quot"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/txt_quot_three"
                    android:textColor="@color/launch_app_bar"

                    android:textSize="@dimen/div_text_size"

                    android:text="1"
                    android:layout_width="@dimen/txt_width_apply_facts_div_quot"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/txt_quot_four"
                    android:textColor="@color/launch_app_bar"

                    android:textSize="@dimen/div_text_size"

                    android:text="1"
                    android:layout_width="@dimen/txt_width_apply_facts_div_quot"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/txt_quot_five"
                    android:textColor="@color/launch_app_bar"

                    android:textSize="@dimen/div_text_size"
                    android:text="1"
                    android:layout_width="@dimen/txt_width_apply_facts_div_quot"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/txt_quot_six"

                    android:textSize="@dimen/div_text_size"
                    android:textColor="@color/launch_app_bar"

                    android:text=""
                    android:layout_width="@dimen/txt_width_apply_facts_div_quot"
                    android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/txt_quot_seven"
                        android:textColor="@color/launch_app_bar"

                        android:textSize="@dimen/div_text_size"

                        android:text=""
                        android:layout_width="@dimen/txt_width_apply_facts_div_quot"
                        android:layout_height="wrap_content" />
                </LinearLayout>



            <LinearLayout
                android:id="@+id/ll_first_row"
                android:layout_below="@+id/ll_quotient"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <LinearLayout
